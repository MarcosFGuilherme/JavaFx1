<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="300.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.ViewController">
   <children>
      <Button fx:id="btTest" layoutX="526.0" layoutY="14.0" mnemonicParsing="false" onAction="#onBtTestAction" text="Alert" />
      <Label layoutX="437.0" layoutY="57.0" text="Calculate SUM" />
      <TextField fx:id="txtNumberOne" layoutX="437.0" layoutY="84.0" promptText="First number" />
      <TextField fx:id="txtNumberTwo" layoutX="437.0" layoutY="120.0" promptText="Second number" />
      <Button fx:id="btSum" layoutX="437.0" layoutY="153.0" mnemonicParsing="false" onAction="#onBtSumAction" text="Soma" />
      <Label layoutX="437.0" layoutY="192.0" text="Result = " />
      <Label fx:id="labelResult" layoutX="286.0" layoutY="192.0" />
      <ComboBox fx:id="comboBoxPerson" layoutX="25.0" layoutY="14.0" onAction="#onComboBoxPersonAction" prefWidth="150.0" promptText="Select Person..." />
      <Button fx:id="btAll" layoutX="214.0" layoutY="14.0" mnemonicParsing="false" onAction="#onBtAllAction" text="All" />
   </children>
</AnchorPane>
